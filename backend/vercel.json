{
    "version" : 2,
    "name" : "BlogProject",
    "builds" : [
        {
            "src" : "./index.js",
            "use" : "@vercel/node"
        }
    ],
    "routes" : [
        {
            "src" : "/(.*)",
            "dest" : "/"
        }
    ],
    "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ]
}     